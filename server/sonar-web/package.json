{
  "name": "SonarQube",
  "version": "0.0.1",
  "description": "Put your technical debt under control",
  "repository": "SonarSource/sonarqube",
  "license": "LGPL-3.0",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "backbone": "1.2.3",
    "backbone.marionette": "2.4.3",
